                                                            <bbf-olt-vomci:remote-address>obbaa-vproxy</bbf-olt-vomci:remote-address>
                                                            <bbf-olt-vomci:remote-port>8433</bbf-olt-vomci:remote-port>
                                                        </bbf-olt-vomci:grpc-transport-parameters>
                                                    </bbf-olt-vomci:grpc>
